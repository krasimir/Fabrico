body {
    width: 100%;
    height: 100%;
    margin: 0;
    padding: 0;
    font-size: 14px;
    font-family: Tahoma, Verdana;
    line-height: 14px;
    background: url(../img/body.jpg) no-repeat top center;
}
h1 {
    font-weight: normal;
    font-family: Georgia;
    font-size: 35px;
    font-style: italic;
    margin: 0;
    padding: 0;
}
.clear {
    clear: both;
}
.content {
    width: 950px;
    margin: 25px auto;
}
.separator {
    display: block;
    width: 100%;
    height: 1px;
    border-top: solid 1px #B6B6B6;
    margin: 15px 0 15px 0;
}
.error {
    color: #BF0000;
    padding: 10px;
    border: solid 1px #BF0000;
}
.left {
    float: left;
}
.right {
    float: right;
}
a {
    color: #000;
    text-decoration: underline;
}
a:hover {
    color: #808080;
    text-decoration: none;
}
table td {
    vertical-align: top;
}
input, textarea {
    padding: 5px;
}
/* header */
.header {
    margin: 25px 0 25px 0;
    padding: 0;
}
.header h1 {
    padding: 13px 0 0 0;
}
.logo {
    float: left;
    margin: 0 20px 0 0;
}
/* data */

/* footer */
.footer {
    display: block;
    width: 950px;
    margin: 0 auto;
    font-size: 11px;
    color: #B2B2B2;
    padding: 10px 20px 10px 20px;
    border-top: solid 1px #D7D7D7;
}
/* main navigation */
.nav-main {
    width: 950px;
    height: 37px;
    background: url(../img/mainNavBackground.jpg) no-repeat;
    padding: 23px 0 15px 34px;
}
.nav-main a {
    text-decoration: none;
    color: #FFF;
    font-size: 16px;
    float: left;
    margin: 0 20px 0 0;
}
.nav-main a:hover {
    color: #535353;
}
.nav-main .nav-main-separator {
    width: 4px;
    height: 13px;
    float: left;
    display: block;
    background: url(../img/mainNavSeparator.png) no-repeat;
    margin: 2px 20px 0 0;
}
/* sub navigation */
.sub-main {
    margin: 0;
    padding: 0 10px 0 10px;
    border-bottom: solid 3px #999;
}
.sub-main a {
    float: left;
    text-decoration: none;
    background: #999;
    color: #FFF;
    padding: 10px;
    text-transform: uppercase;
    font-size: 11px;
    font-weight: bold;
    border-right: solid 1px #000;
}
.sub-main a:hover {
    color: #000;
}
/* listing */
.listing {
    
}
.listing .header td {
    font-size: 12px;
    color: #7C7C7C;
    font-weight: bold;
    padding: 10px;
    border-bottom: solid 1px #7C7C7C;
    border-left: solid 1px #D6D6D6;
}
.listing .data {
    padding: 10px;
    font-size: 12px;
    border-bottom: solid 1px #7C7C7C;
}
/* adding */
.adding {
    margin: 15px 0 0 0;
    width: 100%;
}
.row-name {
    width: 200px;
    border-top: solid 1px #999;
    border-right: solid 1px #999;
    padding: 5px 10px 10px 5px;
}
.row-worker {
    padding: 10px;
    border-top: solid 1px #D6D6D6;
}
.submit {
    
}
.result {
    padding: 20px 0 20px 5px;
}